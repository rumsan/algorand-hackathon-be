generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Beneficiary {
  id            String  @id @unique @default(cuid())
  email         String  @unique
  name          String
  age           Int
  gender        String
  walletAddress String? @unique
}
