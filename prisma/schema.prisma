generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Beneficiary {
  uuid    String     @default(uuid())
  email String  @unique
  name  String
  age Int
  gender String
  walletAddress String? 
  projectId String 
}

model Voucher {
  uuid    String     @default(uuid())
  voucherName String
  voucherSymbol String @unique
  assetId Int @default(0)
}

model Project {
  uuid    String     @default(uuid())
  name String
  tokenSymbol String @unique
  tokenName String
  adminAddress String[]
  createdAt DateTime @default(now())

}